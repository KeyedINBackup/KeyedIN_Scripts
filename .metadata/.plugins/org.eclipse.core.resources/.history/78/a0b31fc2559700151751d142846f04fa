package sampleTestWork;

public class fibonacci {

	public static void main(String[] args){
		System.out.println("FIBONACCI SERIES");
		int i;
		if((i=0) <=10){
			int a=1;
			int b=a+i;
			int c=a+b;
			System.out.println(+i);
			System.out.println(a);
			System.out.println(b);
			System.out.println(c);
			
			
		}
	}
}
